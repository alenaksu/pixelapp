{
    "name": "pixelapp",
    "private": true,
    "scripts": {
        "start": "parcel public/index.html --open --dist-dir docs",
        "start:snow": "snowpack dev",
        "test": "echo \"This template does not include a test runner by default.\" && exit 1",
        "format": "prettier --write 'src/**/*.[j|t]s'",
        "lint": "prettier --check 'src/**/*.[j|t]s'",
        "build": "rimraf docs && parcel build public/index.html --public-url /pixelapp/ --dist-dir docs",
        "start:rollup": "rimraf dist && rollup --config rollup.config.js --watch & es-dev-server --http2 --root-dir dist --app-index index.html --compatibility none --open"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.10.4",
        "@open-wc/building-rollup": "^1.7.0",
        "@rollup/plugin-image": "^2.0.5",
        "@rollup/plugin-typescript": "^5.0.2",
        "@rollup/plugin-wasm": "^5.0.0",
        "@snowpack/plugin-babel": "^2.0.3",
        "deepmerge": "^4.2.2",
        "es-dev-server": "^1.57.2",
        "parcel": "^2.0.0-beta.1",
        "postcss-modules": "^3.2.0",
        "prettier": "^2.0.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.26.2",
        "rollup-plugin-glsl": "^1.3.0",
        "rollup-plugin-string": "^3.0.0",
        "snowpack": "^2.7.7",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-decorators": "^7.10.5",
        "@spectrum-web-components/bundle": "^0.14.5",
        "@spectrum-web-components/slider": "^0.5.3",
        "@spectrum-web-components/styles": "^0.6.0",
        "@spectrum-web-components/themes": "^0.2.3",
        "@tiny-lit/element": "^2.0.0-alpha.6",
        "@tiny-lit/store": "^2.0.0-alpha.6",
        "lit-element": "^2.3.1"
    },
    "browerslist": [
        "Chrome > 50"
    ]
}
